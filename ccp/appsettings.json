{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Debug",
      "Microsoft.AspNetCore.HttpLogging.HttpLoggingMiddleware": "Debug"
    }
  },
  "AllowedHosts": "*",

  "ReverseProxy": {
   "Routes": {
     "route1" : {
       "ClusterId": "cluster1",
       "Match": {
         "Path": "{**catch-all}"
       }
     }
   },

   "Clusters": {
     "cluster1": {
       "Destinations": {
         "destination1": {
           "Address": "https://api.anthropic.com/"
         }
       }
     }
   }
 }
}
